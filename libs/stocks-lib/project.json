{
  "name": "stocks-lib",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/stocks-lib/src",
  "projectType": "library",
  "tags": [
    "type:shared",
    "scope:stocks"
  ],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/stocks-lib",
        "main": "libs/stocks-lib/src/index.ts",
        "tsConfig": "libs/stocks-lib/tsconfig.lib.json",
        "assets": ["libs/stocks-lib/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/stocks-lib/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
